    "express": "4.21.2",
    "express-validator": "6.15.0",
    "helmet": "6.2.0",
    "jsonwebtoken": "9.0.2",
    "mongoose": "7.8.7",
    "morgan": "1.10.0",
    "node-cron": "3.0.3",
    "onesignal-node": "3.4.0",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "9.27.0",
    "@faker-js/faker": "9.8.0",
    "eslint": "9.27.0",
    "globals": "16.2.0",
    "jest": "29.7.0",
    "nodemon": "2.0.22",
    "supertest": "6.3.4"
  }
