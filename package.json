{
  "name": "stopgalere-api",
  "version": "4.0.1",
  "description": "Modern MVC-style Node.js Express API for Jobs, Resumes, and Cover Letters",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon --watch src --exec \"node src/server.js\"",
    "test": "jest --coverage",
