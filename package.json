    "seed-sync":  "node scripts/seed-sync.js",
    "lint": "eslint \"src/**/*.js\"",
    "lint:fix": "eslint \"src/**/*.js\" --fix"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
